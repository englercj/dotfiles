[user]
    name = Chad Engler
    email = englercj@live.com
[push]
    default = matching
[core]
    excludesfile = ~/.gitignore
[color]
    ui = auto
[alias]
    co = checkout

    last = log -1
    lo = log --stat
    lm = log --stat --author=englercj@live.com --abbrev-commit

    ls = status

    lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
    graph = log --graph --decorate --pretty=oneline --abbrev-commit

    sha1 = rev-parse HEAD
    sha = rev-parse HEAD
    ha = rev-parse --short HEAD
